<div class="col-xs-12">
	<form class="form-horizontal" id="basic-form">
		<div class="form-group">
			<label for="input-first-name" class="col-lg-3 control-label">First Name</label>
			<div class="col-lg-8">
				<input name="firstName" type="text" class="form-control" id="input-first-name" placeholder="Trolly" data-required="true">
			</div>
		</div>
		<div class="form-group">
			<label for="input-last-name" class="col-lg-3 control-label">Last Name</label>
			<div class="col-lg-8">
				<input name="lastName" type="text" class="form-control" id="input-last-name" placeholder="McTroll" data-required="true">
			</div>
		</div>
		<div class="form-group">
			<label for="input-email" class="col-lg-3 control-label">Email</label>
			<div class="col-lg-8">
				<input name="eMail" type="email" class="form-control" id="input-email" placeholder="troll@duke.edu" name="email" data-type="email" data-required="true">
			</div>
		</div>
		<div class="form-group">
			<label for="input-organization" class="col-lg-3 control-label">Organization Name</label>
			<div class="col-lg-8">
				<input name="orgName" type="text" class="form-control" id="input-organization" placeholder="Duke Trolls" data-required="true">
			</div>
		</div>
		<div class="form-group" id="plaza-fundcode">
			<label for="input-fundcode" class="col-lg-3 control-label">Fund Code</label>
			<div class="col-lg-8">
				<input name="fundCode" type="text" class="form-control" id="input-fundcode" placeholder="xxx-xxxx" data-required="true">
			</div>
		</div>
		<div class="form-group">
			<label for="input-cell" class="col-lg-3 control-label">Cell Phone Number</label>
			<div class="col-lg-8">
				<input name="cellNumber" type="tel" class="form-control" id="input-cell" placeholder="(919) LOL-DUKE" data-required="true">
			</div>
		</div>
	</form>
</div>
<div class="col-xs-6 col-lg-6 pull-right next" id="basic-info-next">
	<h3>Next<i class="icon-double-angle-down next-arrow"></i></h3>
</div>